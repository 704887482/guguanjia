<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!--引入数据库配置文件以解耦-->
    <properties resource="db.properties"/>
    <!-- mysql jar 文件位置 -->
    <!--  <classPathEntry location="D:\.m2\repository\mysql\mysql-connector-java\5.1.35\mysql-connector-java-5.1.35.jar" />

  -->


    <context id="testTables" targetRuntime="MyBatis3">
        <!--mapper插件-->
        <plugin type="tk.mybatis.mapper.generator.MapperPlugin">
            <property name="mappers" value="tk.mybatis.mapper.common.Mapper"/>
            <property name="caseSensitive" value="true"/>
        </plugin>

        <commentGenerator>
            <!-- 是否去除自动生成的注释，true：是；false:否 -->
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        <!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
        <jdbcConnection driverClass="${druid.driverClassName}"
                        connectionURL="${druid.url}" userId="${druid.username}"
                        password="${druid.password}">
        </jdbcConnection>
        <!--Oracle数据库的连接信息-->
        <!-- <jdbcConnection driverClass="oracle.jdbc.OracleDriver"
            connectionURL="jdbc:oracle:thin:@127.0.0.1:1521:yycg"
            userId="yycg"
            password="yycg">
        </jdbcConnection> -->

        <!-- 默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer，为 true时把JDBC DECIMAL 和
            NUMERIC 类型解析为java.math.BigDecimal -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <!-- targetProject:生成PO类的位置 -->
        <javaModelGenerator targetPackage="cn.nyse.entity"
                            targetProject="src/main/java">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false" />
            <!-- 从数据库返回的值被清理前后的空格 -->
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        <!-- targetProject:mapper映射文件生成的位置 -->
        <sqlMapGenerator targetPackage="cn.nyse.mapper"
                         targetProject="src/main/java">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false" />
        </sqlMapGenerator>
        <!--
        targetPackage：mapper接口生成的位置，遵循MyBatis规范，让mapper.xml
        和mapper.java在同一目录下
        ANNOTATEDMAPPER:注解方式
        XMLMAPPER：xml方式
         -->
        <javaClientGenerator type="ANNOTATEDMAPPER"
                             targetPackage="cn.nyse.mapper"
                             targetProject="src/main/java">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false" />
        </javaClientGenerator>
        <!-- 指定数据库表 -->
        <table tableName="${jdbc.table.items}" domainObjectName="AppVersion" ></table>
        <table tableName="${jdbc.table.items2}" domainObjectName="Demand" ></table>
        <table tableName="${jdbc.table.items3}" domainObjectName="Detail" ></table>
        <table tableName="${jdbc.table.items4}" domainObjectName="EnvironmentalCompany" ></table>
        <table tableName="${jdbc.table.items5}" domainObjectName="ExaminationStandard" ></table>
        <table tableName="${jdbc.table.items6}" domainObjectName="Examine" ></table>
        <table tableName="${jdbc.table.items7}" domainObjectName="IndustryFrom" ></table>
        <table tableName="${jdbc.table.items8}" domainObjectName="ExamOptions" ></table>
        <table tableName="${jdbc.table.items9}" domainObjectName="Qualification" ></table>
        <table tableName="${jdbc.table.items10}" domainObjectName="Statute" ></table>
        <table tableName="${jdbc.table.items11}" domainObjectName="SysArea" ></table>
        <table tableName="${jdbc.table.items12}" domainObjectName="SysDict" ></table>
        <table tableName="${jdbc.table.items13}" domainObjectName="SysLog" ></table>
        <table tableName="${jdbc.table.items14}" domainObjectName="SysOffice" ></table>
        <table tableName="${jdbc.table.items15}" domainObjectName="SysResource" ></table>
        <table tableName="${jdbc.table.items16}" domainObjectName="SysRole" ></table>
        <table tableName="${jdbc.table.items17}" domainObjectName="SysUser" ></table>
        <table tableName="${jdbc.table.items18}" domainObjectName="Transfer" ></table>
        <table tableName="${jdbc.table.items19}" domainObjectName="TransportLine" ></table>
        <table tableName="${jdbc.table.items20}" domainObjectName="UserBoundDevice" ></table>
        <table tableName="${jdbc.table.items21}" domainObjectName="Waste" ></table>
        <table tableName="${jdbc.table.items22}" domainObjectName="WasteType" ></table>
        <table tableName="${jdbc.table.items23}" domainObjectName="WorkOrder" ></table>
        <!-- <table schema="" tableName="sys_user"></table>
        <table schema="" tableName="sys_role"></table>
        <table schema="" tableName="sys_permission"></table>
        <table schema="" tableName="sys_user_role"></table>
        <table schema="" tableName="sys_role_permission"></table> -->

        <!-- 有些表的字段需要指定java类型
         <table schema="" tableName="">
            <columnOverride column="" javaType="" />
        </table> -->
    </context>
</generatorConfiguration>
